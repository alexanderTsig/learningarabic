#sidebar {
	padding:10px 0;
}

#maincontent {
	padding:1.5em 2.2em;
	border-left:1px solid #dadada;
	background-color:white;
}

span[dir=rtl] {
	display:none;
	font-size:18px;
}

.accordion-group {
	background:#fff;
	margin-bottom:10px;
}

.accordion-heading {
	position:relative;
}

.dropdown {
	margin-top:auto;
	margin-bottom:auto;
	position:absolute;
	right:5px;
	top:0;
	bottom:0;
	max-height:100%;
	max-width:100%;
}

.dropdown.active {
	-moz-transform:rotate(90deg);
}

a.accordion-toggle {
	font:1em 'Segoe UI','Lucida Grande',Verdana,Arial,Helvetica,sans-serif;
}

.accordion-body a {
	text-decoration:none;
	color:#000;
}

.accordion-body a.active {
	color:#0088CC;
}

.panel {
	background:#f7f7fd;
	margin-bottom:1.12em;
	border:1px solid #3a4450;
}

.panel > header {
	background-color:#3a4450;
	padding:0.5em;
	color:#fff;
	margin-bottom:0.5em;
}

.panel > ul {
	padding:0 1em;
}

.panel small {
	color:#555;
}

table.lessons td {
	vertical-align:middle;
}

table.lessons > tbody > tr > td:nth-child(2) {
	width:33%;
}

table.lessons div.status {
	width:5em;
	float:right;
	text-align:center
}

table.lessons span.score {
	float:left;
	margin-right:0.5em;
	width:2.5em;
	text-align:right
}

table.lessons div.progress {
	margin:0;
}

table.lessons tr.locked a {
	color:#aaa;
}

table.lessons tr.locked a:hover {
	text-decoration:none;
}

#navbar div.locked > a {
	color:#aaa;
	cursor:default;
}

#teachers {
	margin:0 !important;
	text-align:center;
}

#teachers > li {
	display:inline-block;
}

#teachers > li:nth-child(1) {
	/* margin-right:15px; */
	margin-right:6px;
}

#teachers > li > img {
	display:block;
	margin-bottom:5px;
	border:1px solid #ddd;
}

#teachers > li.unlocked > img {
	cursor:pointer;
}

#teachers > li.unlocked > img:hover {
	opacity:0.85;
}

#teachers > li.locked {
	color:#0088cc;
	text-decoration:underline;
	cursor:default;
}

#sidebar > h4 {
	text-align:center;
	margin-bottom:0.3em;
}

#sidebar > ul {
	text-align:center;
}

.canvas {
	margin:0 auto;
}

.canvas-login {
	width:450px;
}

.canvas-login > #logo {
	border-bottom:0px solid #7f7f7f;
	margin:30px auto;
	width:250px;
}

.canvas-login > #logo img {
	display:block;
	margin:0 auto;
}

.canvas-login > header {
	height:50px;
	line-height:50px;
	border: 1px solid #ccc;
	background-color: #f6f6f6;
	background-image: linear-gradient(to bottom, #fafafa, #f2f2f2);
	background-repeat: repeat-x;
	border-radius:4px 4px 0 0;
	padding:0 10px;
}

.canvas-login > header > h3 {
	line-height:50px;
	margin:0;
	text-transform:uppercase;
	font-size:1.2em;
	color:#555;
	letter-spacing:0.2pt;
	float:left;
}

.canvas-login > header > .btn-toolbar {
	float:right;
	margin:0;
}

form#login {
	position:relative;
	top:-1px;
	background:#fff;
	border:solid 1px #ccc;
	border-radius:0 0 4px 4px;
	padding:18px 20px 0 20px;
}

.canvas-login .form-actions div.span9 {
	/* FIXME: there ought to be a better way of vertically aligning the text to the log in button ... */
	padding-top:10px;
}

.canvas-login > #footer {
	text-align:center;
	color:#999;
}
