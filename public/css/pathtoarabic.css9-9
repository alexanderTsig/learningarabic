#masthead {
	background: #2A333B;
	color: #FFFFFF;
	height: 48px;
	padding: 20px 0;
}

#masthead h1 {
	line-height:80px;
	vertical-align:middle;
	margin-right:15px;
}

/* #masthead > img {
	display:block;
	position:relative;
	margin-left:auto;
	margin-right:10px;
	top:47px;
