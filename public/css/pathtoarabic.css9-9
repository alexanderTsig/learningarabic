.tiles {
	visibility:hidden;
	overflow:hidden;
}

.tile {
	float:left;
	color:#fff;
	border-width:5px;
	border-style:solid;
	border-color:#fff;
	-moz-box-sizing:border-box;
	-webkit-box-sizing:border-box;
	box-sizing: border-box;
}

.tile:hover {
	cursor:pointer;
}

.tile.clickable:hover, .tiles.selectable > .tile:hover {
	border-color:rgba(255, 255, 255, .4);
}

.tile.locked:hover {
	border-color:#fff !important;
}

.tile.locked:hover {
	cursor:default;
}

/* .tiles.selectable > .tile:hover {
	border:5px solid #2669c0;
} */

.tiles.selectable > .tile.selected {
	background-image:url("/img/metro-corner-tick.png");
	background-repeat:no-repeat;
	background-position:right top;
	border-color:#2669c0 !important;
}

.tile > .content {
	display:table-cell;
	vertical-align:middle;
	padding:15px 15px 0 15px;
	-moz-box-sizing:border-box;
	-webkit-box-sizing:border-box;
	box-sizing: border-box;
}

.tile > .content > p {
	margin:1em 0;
	line-height:1.5em;
}

.tile a {
	color:#fff;
	text-decoration:none;
}

.tile a:hover {
	
	color:#2e8bcc;
	text-decoration:none;
	
}


